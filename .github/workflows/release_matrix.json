{
  "build": ["linux", "windows", "armv7", "macos-amd64", "macos-arm64"],
  "include": [
    {
      "build": "linux",
      "os": "ubuntu-latest",
      "target": "x86_64-unknown-linux-gnu",
      "binary": "remindee-bot"
    },
    {
      "build": "armv7",
      "os": "ubuntu-latest",
      "target": "armv7-unknown-linux-gnueabihf",
      "binary": "remindee-bot",
      "is_arm": true
    },
    {
      "build": "windows",
      "os": "ubuntu-latest",
      "target": "x86_64-pc-windows-gnu",
      "binary": "remindee-bot.exe",
      "zip_archive": true
    },
    {
      "build": "macos-amd64",
      "os": "macos-latest",
      "target": "x86_64-apple-darwin",
      "binary": "remindee-bot",
      "is_macos": true,
      "zip_archive": true
    },
    {
      "build": "macos-arm64",
      "os": "macos-latest",
      "target": "aarch64-apple-darwin",
      "binary": "remindee-bot",
      "is_macos": true,
      "zip_archive": true
    }
  ]
}
